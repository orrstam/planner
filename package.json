{
  "name": "planner",
  "version": "1.0.0",
  "author": "Jonas",
  "scripts": {
    "start": "concurrently \"npm run watch\" \"npm run assets\" \"yarn client\"",
    "client": "cd client && yarn start",
    "watch": "nodemon ./build/server.js",
    "assets": "webpack -w --mode development --config webpack.config.js",
    "test": "./node_modules/jest/bin/jest.js"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "server"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/tests/**/*.test.(ts|js)"
    ],
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/coverage/**",
      "!**/webpack.config.js"
    ]
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.3",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.11.1",
    "@types/jest": "^22.2.3",
    "@types/mongoose": "^5.2.5",
    "@types/mongoose-delete": "^0.5.0",
    "@types/mongoose-promise": "^4.5.7",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/passport-local": "^1.0.33",
    "@types/prop-types": "^15.5.5",
    "@types/react-select": "^2.0.1",
    "@types/supertest": "^2.0.4",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "jest": "^23.1.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "supertest": "^3.1.0",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.3.0",
    "ts-node": "^6.0.5",
    "tslint": "^5.10.0",
    "typescript": "^3.1.2",
    "webpack": "4.34.0",
    "webpack-cli": "3.3.4"
  },
  "dependencies": {
    "@types/cors": "^2.8.4",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-async-errors": "^3.1.1",
    "express-http-proxy": "^1.5.1",
    "mobx": "^5.9.4",
    "mobx-react": "^5.0.0",
    "moment": "^2.22.2",
    "mongoose": "^5.2.8",
    "mongoose-delete": "^0.4.0",
    "mongoose-type-email": "^1.0.10",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "react-select": "^2.2.0",
    "strava-v3": "^1.14.0"
  }
}
